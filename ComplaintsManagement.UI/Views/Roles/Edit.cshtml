@model  ComplaintsManagement.UI.Models.TaskResult<ComplaintsManagement.Domain.DTOs.RolesDto>

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-header bg-primary"><h3 class="text-white">Mantenimiento de Roles</h3></div>
    <div class="row card-body">
        <div class="col-lg-12">
            @{
                Html.RenderPartial("_CreateOrUpdate", Model);
            }

        </div>
    </div>
    <div class="row m-3">
        <button class="btn btn-default">@Html.ActionLink("Regresar al listado de Roles", "Index")</button>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
